<table class="table">
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.id %></td>
        <td><%= link_to product.product_name, admins_product_path(product) %></td>
        <td><%= product.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>